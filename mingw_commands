x86_64-w64-mingw32-gcc -std=c99 src/pac_game_jumbo.c -Iinclude -Lwindows/lib -o windows/PacGame/pac-game-win -lmingw32 -lSDL2main -lSDL2 -lSDL2_image -lSDL2_ttf -lSDL2_mixer
x86_64-w64-mingw32-gcc -std=c99 src/editor_jumbo.c -Iinclude -Lwindows/lib -o windows/PacGame/pac-tilemap_editor-win -lmingw32 -lSDL2main -lSDL2 -lSDL2_image -lSDL2_ttf -lSDL2_mixer


x86_64-w64-mingw32-gcc src/pac_game_jumbo.c -Iinclude -Lwindows/lib -o windows/bin/pac-game-win -lmingw32 -lSDL2main -lSDL2 -lSDL2_image

x86_64-w64-mingw32-gcc src/tilemap_editor.c -Iwindows/include -Lwindows/lib -o windows/bin/pac-tilemap-editor-win -lmingw32 -lSDL2main -lSDL2 -lSDL2_image

cmake  -D CMAKE_TOOLCHAIN_FILE=../toolchains/Toolchain-cross-mingw32-linux.cmake -D CMAKE_INSTALL_PREFIX=buildMingw64  -S src -B buildMingw64

cmake  -D CMAKE_C_COMPILER=clang  -S src -B build

x86_64-w64-mingw32-gcc src/pac_game_jumbo.c -Iinclude -Lwindows/lib -o windows/bin/pac-game-win -lmingw32 -lSDL2main -lSDL2 -lSDL2_image -lSDL2_ttf